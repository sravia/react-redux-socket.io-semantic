{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "react/forbid-prop-types": 0
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "mocha": true,
        "jest": true
    }
}